<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:RedFocus.Converters"
                    xmlns:local="clr-namespace:RedFocus.Controls"
                    >
    <Color x:Key="PrimaryBackgroundColor">#181818</Color>
    <Color x:Key="PanelBackgroundColor">#343434</Color>
    <Color x:Key="AccentColor">White</Color>
    <Color x:Key="StaticTextColor">White</Color>
    <Color x:Key="TitleColor">#FFE589</Color>

    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="{StaticResource PrimaryBackgroundColor}"/>
    <SolidColorBrush x:Key="PanelBackgroundBrush" Color="{StaticResource PanelBackgroundColor}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="StaticTextBrush" Color="{StaticResource StaticTextColor}"/>
    <SolidColorBrush x:Key="TitleBrush" Color="{StaticResource TitleColor}"/>
</ResourceDictionary>
